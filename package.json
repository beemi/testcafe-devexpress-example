{
  "name": "testcafe-devexpress-example",
  "version": "1.0.0",
  "description": "TestCafe DevExpress example tests",
  "main": "",
  "scripts": {
    "test:chrome": "testcafe $NODE_DEBUG_OPTION chrome testcafe/integration/*.test.js --assertion-timeout 10000 --disable-page-caching --reporter spec,html:reports/testcafe/index.html",
    "test:chrome:headless": "testcafe $NODE_DEBUG_OPTION chrome:headless testcafe/integration/*.test.js --assertion-timeout 10000 --disable-page-caching --reporter spec,html:/tmp/test-results/testcafe/index.html"
  },
  "repository": "git@github.com:beemi/testcafe-devexpress-example.git",
  "author": {
    "name": "Beemi Raja",
    "email": "beemi.raja@gmail.com"
  },
  "license": "MIT",
  "keywords": [
    "testing",
    "testcafe",
    "ui",
    "dom",
    "integration",
    "functional",
    "end-to-end",
    "e2e",
    "Techical test"
  ],
  "dependencies": {
    "@testing-library/testcafe": "^4.3.1",
    "dotenv": "^8.2.0",
    "faker": "^5.1.0",
    "testcafe": "^1.10.1",
    "testcafe-reporter-html": "^1.4.6"
  }
}
